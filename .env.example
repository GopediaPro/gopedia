# Database Configuration
DATABASE_URL=postgresql+asyncpg://user:password@host:port/database

# Application Configuration
APP_ENV=development
LOG_LEVEL=INFO

# gRPC Plugin Defaults
DEFAULT_PLUGIN_TIMEOUT=5

# GitHub API Configuration (for seeding mock data)
# These values are used as defaults when running seed_github_mock_data.py
# You can override them with command-line arguments
GITHUB_OWNER=octocat
GITHUB_REPO=Hello-World
GITHUB_BRANCH=main
# Optional: GitHub personal access token for higher rate limits
# Get your token from: https://github.com/settings/tokens
GITHUB_TOKEN=
